{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:flowtype/recommended"
  ],
  "plugins": ["flowtype"],
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "react/jsx-filename-extension": 0
  },
  "globals": {
    "__DEV__": false
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
